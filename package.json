{
  "name": "my-doctor-ssr",
  "version": "1.0.0",
  "description": "my doctor ssr ",
  "private": false,
  "dependencies": {
    "egg": "^2.2.1",
    "egg-scripts": "^2.5.0",
    "egg-mongoose": "^3.1.1",
    "egg-view-ejs": "^2.0.0",
    "egg-view-nunjucks": "^2.2.0",
    "moment": "^2.22.2",
    "uuid": "^3.3.2",
    "vue": "^2.5.21",
    "vue-router": "^3.0.2",
    "vue-server-renderer": "^2.5.21",
    "vuex": "^3.0.1",
    "lru-cache": "^5.1.1"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.0.0",
    "egg-bin": "^4.3.5",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.14.0",
    "eslint": "^4.11.0",
    "eslint-config-egg": "^6.0.0",
    "webstorm-disable-index": "^1.2.0",
    "url-loader": "^1.1.2",
    "vue-loader": "^15.4.2",
    "vue-template-compiler": "^2.5.21",
    "webpack": "^4.27.1",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-merge": "^4.1.5",
    "webpack-node-externals": "^1.7.2",
    "css-loader": "^2.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "rimraf": "^2.6.2",
    "cross-env": "^5.2.0",
    "autoprefixer": "^9.4.2"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-my-doctor-ssr",
    "stop": "egg-scripts stop --title=egg-server-my-doctor-ssr",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod",
    "build": "rimraf ./app/public/mydoctor_ssr && npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.config.js --progress --hide-modules"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rhymedys/mydoctor_ssr.git"
  },
  "author": "rhymedys",
  "license": "MIT"
}
