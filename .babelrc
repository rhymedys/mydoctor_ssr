{
  "env": {
    "node": {
      "presets": [
        ["env", {
          "modules": false,
          "targets": {
            "node": "current"
          }
        }]
      ],
      "plugins": [
        "transform-object-rest-spread",
        "syntax-dynamic-import"
      ]
    },
    "web": {
      "presets": [
        ["env", {
          "modules": false,
          "targets": {
            "browsers": ["last 2 versions", "safari >= 7"]
          }
        }]
      ],
      "plugins": [
        "transform-object-rest-spread",
        "syntax-dynamic-import",
        "transform-object-assign",
        [
          "component",
          [{
              "libraryName": "element-ui",
              "styleLibraryName": "theme-chalk"
            },
            {
              "libraryName": "mint-ui",
              "style": true
            }
          ]
        ]
      ]
    }
  },
  "comments": false
}